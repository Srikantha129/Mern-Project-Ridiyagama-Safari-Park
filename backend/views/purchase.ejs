<style>
body {
  font-family: Arial;
}

* {
  box-sizing: border-box;
}

form.example input[type=text] {
  padding: 10px;
  font-size: 17px;
  border: 1px solid grey;
  float: left;
  width: 80%;
  background: #f1f1f1;
}

form.example button {
  float: left;
  width: 20%;
  padding: 10px;
  background: #1a7833;
  color: white;
  font-size: 17px;
  border: 1px solid grey;
  border-left: none;
  cursor: pointer;
}

form.example button:hover {
  background: #095d0f;
}

form.example::after {
  content: "";
  clear: both;
  display: table;
}

label {
            color: darkgreen;
        }

        .card {
          box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
          transition: 0.3s;
          width: 40%;
          margin-left: auto;
          margin-right: auto;
          background-color: rgb(29, 117, 27);
          background-image: linear-gradient(to bottom right, rgb(37, 113, 26), rgb(243, 243, 234));
          border-color: #19471c;
          
        }
        
        .card:hover {
          box-shadow: 0 8px 20px 0 rgba(0,0,0,0.2);
          
        }
        
        .container {
          padding: px 20px;
        }

        .size {
            padding: 20px 40px;
            text-align: left;
        }
        .row {
            margin-right: 50px;
        }
        .search {
           align-items: center;
        }
        .button {
            align-items: center;
        }
        .text {
            text-align: center;
        }
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            background-color: #7cda89;
            }

            li {
            float: left;
            }

            li a {
            display: block;
            color: rgb(6, 6, 6);
            text-align: center;
            padding: 14px 16px;
            text-decoration: none;
            }

            li a:hover {
            background-color: rgb(160, 220, 165);
            }

</style>
