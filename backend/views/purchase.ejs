  <ul>
    <li><a class="active" href="/admin/Login.html">Home</a></li>
    <li><a href="#news">Dashboard</a></li>
</ul>

  <br>
  <div style="padding-top: 100px;">
    <br>
    <div class="card ">
      <div class="container mt-5">
        <div class="text">
          <h5>See description of the payment</h5>
        </div>
          <div class="card-body flex-column align-items-center">
            <form class="example" action="/purchasee" style="margin:auto;max-width:400px" method="POST">
              <label for="Name">Search Email address</label>
              <input type="text" placeholder="Search.." name="search">
              <button type="submit"><i class="fa fa-search"></i></button>
            </form>

            <br><br><br>
            <div class="button text">
              <h5>See full description of all payments</h5><br>
              <a href="https://dashboard.stripe.com/test/payments" class="btn btn-success" role="button">View Details &rarr;</a>
            </div>
            <br><br>
          </div>
      </div>
    </div>
  </div>
</div>

